{
  "name": "winfbot",
  "version": "1.1.0",
  "main": "dist/index.js",
  "scripts": {
    "dev": "NODE_ENV=development tsx watch src/index.ts",
    "start:dev": "NODE_ENV=development node dist/index.js",
    "start:prod": "NODE_ENV=production node dist/index.js",
    "build": "tsup 'src/**/*.ts' --format cjs --out-dir dist --clean",
    "deploy:dev": "NODE_ENV=development tsx src/deploy-commands.ts",
    "deploy:prod": "NODE_ENV=production tsx src/deploy-commands.ts",
    "migrate": "tsx src/scripts/moveDatabase.ts",
    "format:check": "prettier --check .",
    "format": "prettier --write src/**/*.{ts,js,json}"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "discord-arts": "^0.7.1",
    "discord.js": "^14.21.0",
    "dotenv": "^17.2.0",
    "moment": "^2.30.1",
    "moment-timezone": "^0.6.0",
    "mongodb": "^6.17.0",
    "mongoose": "^8.16.4"
  },
  "devDependencies": {
    "@types/moment-timezone": "^0.5.13",
    "@types/node": "^24.0.15",
    "@types/prettier": "^2.7.3",
    "prettier": "^3.6.2",
    "tsup": "^8.0.0",
    "tsx": "^4.20.3",
    "typescript": "^5.0.0"
  }
}
